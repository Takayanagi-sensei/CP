#include <stdio.h>
int main(){
    int n,count=0,min,max;
    scanf("%d",&n);
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        scanf("%d",&arr[i]);
    }
    
        max=arr[0];
        min=arr[0];
    for (int i = 0; i < n; i++)
    {
            if (max<arr[i])
            {
                max=arr[i];
                count++;
            }
            if (min>arr[i])
            {
                min=arr[i];
                count++;
            }
 
    }
    printf("%d\n",count);
 
 
}
