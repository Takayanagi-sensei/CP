#include <stdio.h>
int main(){
    int n,count=0,j=1,a,b;
    scanf("%d",&n);
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        arr[i]=j;
        j++;
    }
    
    for (int i = 0; i < 2; i++)
    {
        scanf("%d",&a);
        for (int t = 0; t < a; t++)
        {
            scanf("%d", &b);
            for (int j = 0; j < n; j++)
            {
                if (arr[j]==b)
                {
                    arr[j]=0;
                    //count++;
                
                }
                
            }

            
            
        }
        
        
        
    }
    for (int i = 0; i < n; i++)
        {
            if (arr[i])
            {
                count=1;
            }
        }
    if (count==0)
    {
        printf("I become the guy.\n");
    }
    else 
        printf("Oh, my keyboard!\n");

    

}
